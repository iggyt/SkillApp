<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:viz="sap.viz.ui5.controls"
		controllerName="controller.ReportingView" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page>
		<customHeader>
            <Bar>
                <contentLeft>
                    <Button id="idBackButton" icon="sap-icon://nav-back" press="goBack" tooltip="Back" />
                    <Button id="idHomeButton" icon="sap-icon://home" press="goHome" tooltip="Home" />
                </contentLeft>
                <contentMiddle>
                    <Label text="Reporting" />
                </contentMiddle>
                <contentRight>
                    <Button id="idPrintButton" icon="sap-icon://print" press="printView" tooltip="Print" />
                    <Button id="idExportButton" icon="sap-icon://download" press="exportToExcel" tooltip="Export to Excel" />
                </contentRight>
            </Bar>
		</customHeader>
		<content>
			<viz:VizFrame xmlns="sap.viz" id="idChart">
			
			</viz:VizFrame>
		</content>
	</Page>
</core:View>